                  <div class="card card-id-{{feeds.id}}">
                   <div class="card-header">
                      <strong>{{feeds.feedName}}</strong>
                      <span class="label label-info"><a href="/edit/rss/{{feeds.id}}/edit">Edit</a></span>
                   </div>
                   <div class="card-body">
                      <div class="list-group">
                         {{feed.getFeed(url, 7)}}
                      </div>
                   </div>
                  </div>
                </div>
                {% endif %}
